{
  "name": "team-up",
  "version": "1.0.0",
  "private": true,
  "description": "Team Up website that allow members to find other people to join them in a sports activity",
  "main": "server.js",
  "scripts": {
    "prestart": "npm-run-all --parallel security-check linter",
    "start": "npm-run-all --parallel server",
    "server": "node ./bin/www -p 8080 webpack",
    "build": "webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "linter": "esw --ignore-path .gitignore --fix ./",
    "security-check": "nsp check"
  },
  "repository": {
    "type": "git",
    "url": "git@csil-git1.cs.surrey.sfu.ca:rtanzeel/Team_Up.git"
  },
