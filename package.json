  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "express-validator": "^3.2.0",
    "helmet": "^3.6.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.0",
    "nconf": "^0.8.4",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.0-rc.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "browser-sync-webpack-plugin": "^1.1.4",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.9.0",
    "eslint-watch": "^3.1.2",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "open": "^0.0.5",
    "webpack": "^3.0.0"
  }
